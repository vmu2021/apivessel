<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
	xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">

	<mapped-superclass class="mdef.Producto"	access="FIELD">
		<attributes>
			<many-to-one name="catalogo" target-entity="com.vesselapi.entidades.CatalogoConId">
				<join-column name="CATALOGO" />
			</many-to-one>
		
		</attributes>
	</mapped-superclass>

</entity-mappings>
